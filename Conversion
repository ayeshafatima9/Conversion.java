public class Conversion{
public static void main(String[] args){
long min,years,days;
min=3456789;
System.out.println("Conversion of minute to years");
years=min/(60*24*365);
System.out.println(min+" is "+years+" years ");
System.out.println("Conversion of minute to days");
days=(min/60/24)%365;
System.out.println(min+" is "+days+" days");
}
}
